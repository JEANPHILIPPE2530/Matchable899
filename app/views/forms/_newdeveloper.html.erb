  <div class="container">
    <h1> Build Your Profile</h1>
    <i class='bx bx-user-circle'></i>
    <p><strong>Biography</strong></p>
    <%= simple_form_for(@developer) do |f| %>
    <%= f.input :first_name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :website %>
    <%= f.input :zoom %>

    <%= f.submit %>
    <% end %>
  </div>
